{
  "name": "neeto-jwt",
  "version": "1.0.2",
  "type": "module",
  "engines": {
    "node": ">=22.13"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "NODE_NO_WARNINGS=1 node --loader ts-node/esm -i -e \"import NeetoJWT from './src/index.ts'; globalThis.NeetoJWT = NeetoJWT;\"",
    "test": "vitest run"
  },
  "dependencies": {
    "jsonwebtoken": "9.0.2"
  },
  "devDependencies": {
    "@types/node": "22.13.14",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "vitest": "3.1.1"
  }
}
